{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@teamkeel/sdk": ["./.build/sdk"],
      "@teamkeel/testing": ["./.build/testing"]
    }
  },
  "include": ["**/*"]
}
